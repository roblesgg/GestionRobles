<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <ImageView fx:id="imgFondo" fitHeight="438.0" fitWidth="712.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../img/imgFondo.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgLogo" fitHeight="247.0" fitWidth="228.0" layoutX="242.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../img/imgLogo.png" />
         </image>
      </ImageView>
      <Button fx:id="btnEntrar" layoutX="278.0" layoutY="249.0" mnemonicParsing="false" onAction="#abrirMenu" style="-fx-background-radius: 40; -fx-background-color: #cab8a4;" text="Entrar">
         <font>
            <Font name="Calibri Bold" size="40.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
